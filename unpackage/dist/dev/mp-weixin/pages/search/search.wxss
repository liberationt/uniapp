page {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  background-color: #f5f5f5;
}
.home {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 100%;
}
.search-list .label-box {
  margin-bottom: 10px;
  background: #fff;
}
.search-list .label-box .label-header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 14px;
  color: #666;
  padding: 10px 15px;
  border-bottom: 1px solid #f5f5f5;
}
.search-list .label-box .label-header .label-title {
  color: #f07373;
}
.search-list .label-box .label-header .label-clear {
  color: #30b33a;
  font-weight: bold;
}
.search-list .label-box .label-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 15px;
  padding-top: 0px;
}
.search-list .label-box .label-list .label-item {
  padding: 3px 12px;
  border: 1px solid #666;
  border-radius: 5px;
  margin-top: 12px;
  margin-right: 10px;
  font-size: 14px;
  color: #666;
}
.search-list .label-box .no-data {
  height: 200px;
  color: #666;
  font-size: 14px;
  text-align: center;
  line-height: 200px;
}

